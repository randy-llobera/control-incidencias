{
  "name": "control-comportamiento",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.9.4",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write .",
    "types": "supabase gen types typescript --project-id ztnpeivtdpvzaeptwrib --schema public > src/types/supabase.ts",
    "prebuild": "sh -c 'if [ -n \"$SUPABASE_ACCESS_TOKEN\" ] && [ -n \"$SUPABASE_PROJECT_REF\" ]; then npm run types; else echo Skipping Supabase typegen; fi'",
    "db:ensure-admin": "node scripts/ensure-local-admin.js",
    "db:seed:local": "node scripts/run-seed.js",
    "db:reset:local": "supabase db reset && npm run db:ensure-admin && npm run db:seed:local"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tailwindcss/vite": "^4.0.0",
    "next": "^15.5.4",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "eslint": "^8",
    "eslint-config-next": "15.1.0",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "supabase": "^2.45.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
